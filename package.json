{
  "name": "three-es6-s3-boilerplate",
  "version": "1.0.4",
  "private": true,
  "engines": {
    "node": ">=6.1.0",
    "npm": ">=3.0.0"
  },
  "config": {
    "ports": {
      "server": 50000
    },
    "targets": [
      "dashboard",
      "app"
    ],
    "aws": {
      "buckets": [
        "ADD YOUR STAGING BUCKET NAME",
        "ADD YOUR PRODUCTION BUCKET NAME"
      ],
      "s3": {
        "region": "us-west-2",
        "sslEnabled": true,
        "apiVersion": "2016-05-01",
        "params": {
          "ACL": "public-read"
        }
      }
    }
  },
  "scripts": {
    "build": "rimraf dist/dashboard && webpack --config webpack.config.prod.js --bail --progress --profile",
    "dev": "webpack-dev-server --config webpack.config.dev.js --watch --colors --inline --progress",
    "bump": "node scripts/bump.js",
    "release": "npm run bump && npm run build && node scripts/release.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "three": "latest",
    "babel-plugin-transform-exponentiation-operator": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "boxcutter": "^1.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "inquirer": "^1.0.2",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.7.0",
    "npm-run-all": "^2.1.0",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "s3": "^4.4.0",
    "sass-loader": "^3.2.0",
    "semver": "^5.1.0",
    "shelljs": "^0.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-combine-loaders": "^2.0.0",
    "webpack-dev-server": "^1.14.1"
  }
}